    <!-- http://docs.oasis-open.org/ns/tal#Vandal -->

    <owl:Class rdf:about="http://docs.oasis-open.org/ns/tal#Vandal">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/tal#HostileThreatActorType"/>
        <commonActions xml:lang="en">Network/computing disruption, web hijacking, malware</commonActions>
        <description xml:lang="en">Derives thrills from intrusion or destruction of property, without strong agenda</description>
    </owl:Class>
    


