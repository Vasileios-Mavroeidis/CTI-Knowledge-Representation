        <commonActions xml:lang="en">Theft of hardware goods or IP, PII, or business data</commonActions>
        <description xml:lang="en">Opportunistic individual with simple profit motive</description>
    </owl:Class>
    


    <!-- http://docs.oasis-open.org/ns/tal#ThreatActorType -->

    <owl:Class rdf:about="http://docs.oasis-open.org/ns/tal#ThreatActorType"/>
    


    <!-- http://docs.oasis-open.org/ns/tal#UntrainedEmployee -->

    <owl:Class rdf:about="http://docs.oasis-open.org/ns/tal#UntrainedEmployee">
        <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/tal#NonHostileThreatActorType"/>
